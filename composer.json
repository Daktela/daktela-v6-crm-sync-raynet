{
    "name": "daktela/crm-sync-raynet",
    "description": "Raynet CRM adapter for Daktela CRM Sync SDK",
    "type": "library",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "/daktela-v6-php-crm-sync"
        }
    ],
    "require": {
        "php": ">=8.2",
        "daktela/crm-sync": "dev-main",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^2.0||^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Daktela\\CrmSync\\Raynet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daktela\\CrmSync\\Raynet\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
